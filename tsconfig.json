{
	"compilerOptions": {
		"strict": true,
		"moduleResolution": "node",
		"module": "esnext",
		"target": "es2021",
		"lib": [
			"esnext",
			"esnext.array",
			"esnext.asynciterable",
			"esnext.intl",
			"esnext.symbol"
		],
		"sourceMap": false,
		"inlineSourceMap": true,
		"inlineSources": true,
		"sourceRoot": "src",
		"outDir": "dist",
		"declaration": false,
		"removeComments": false,
		"alwaysStrict": true,
		"allowSyntheticDefaultImports": true,
		"pretty": true
	}
}
